<?xml version="1.0" encoding="UTF-8"?>
<configuration  xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
   xmlns='http://www.mitre.org/medfacts'
   xsi:schemaLocation='http://www.mitre.org/medfacts rules_schema.xsd'>
    <class>
        <class-name>patient_ref</class-name>
        <class-value>the patient</class-value>
        <class-value>him</class-value>
        <class-value>her</class-value>
        <class-value />
    </class>
    <class>
        <class-name>direction_ref</class-name>
        <class-value>for</class-value>
        <class-value>against</class-value>
        <class-value />
    </class>
    <rule name="rule_out" tag="PREN">rule ${patient_ref} out</rule>
    <rule tag="PREN">did ${rule_out} ${direction_ref}</rule>
</configuration>
