<?xml version="1.0" encoding="UTF-8"?>
<m:configuration  xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
   xmlns:m='http://www.mitre.org/medfacts'
   xsi:schemaLocation='http://www.mitre.org/medfacts rules_schema.xsd'>
    <m:class>
        <m:class-name>patient_ref</m:class-name>
        <m:class-value>the patient</m:class-value>
        <m:class-value>him</m:class-value>
        <m:class-value>her</m:class-value>
        <m:class-value />
    </m:class>
    <m:class>
        <m:class-name>direction_ref</m:class-name>
        <m:class-value>for</m:class-value>
        <m:class-value>against</m:class-value>
        <m:class-value />
    </m:class>
    <m:rule tag="PREN">rules <class-ref idref="patient_ref"/> out</m:rule>
    <m:rule name="rule_out" tag="PREN">rule <class-ref idref="patient_ref"/> out</m:rule>
    <m:rule tag="PREN">did <class-ref idref="rule_out_ref"/> <class-ref idref="direction_ref"/></m:rule>

    <!--
    <m:rule tag="PREN">did ${rule_out}</m:rule>
    <m:rule tag="PREN">did ${rule_out} for</m:rule>
    <m:rule tag="PREN">did ${rule_out} against</m:rule>
    <m:rule tag="PREN">can ${rule_out}</m:rule>
    <m:rule tag="PREN">can ${rule_out} for</m:rule>
    <m:rule tag="PREN">can ${rule_out} against</m:rule>
    --%>
</m:configuration>
