            </goals>
            <properties>
                <exec.args>${profiler.args} -classpath %classpath org.mitre.medfacts.i2b2.cli.BatchRunner --base-dir="C:/Documents and Settings/wellner/MedFactExtraction/runs-towards-final/" --train=wellner-train --decode=wellner-test --features-file=featureFile2</exec.args>
                <profiler.action>profile</profiler.action>
                <exec.executable>${profiler.java}</exec.executable>
            </properties>
        </action>
    </actions>
